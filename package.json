    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fastify-tsconfig": "^1.0.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "prettier": "^2.2.1",
