{
  "name": "test-app-backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "engines": {
    "node": "12.18.2"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "tsc": "tsc",
    "test": "npm run build:ts && tsc -p test/tsconfig.test.json && tap test/**/*.test.ts",
    "start": "npm run build:ts && fastify start -l info dist/app.js",
    "build:ts": "npm run tsc",
    "dev": "npm run tsc && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\"  \"tsc -w\" \"fastify start -w -l info -P dist/app.js\""
  },
