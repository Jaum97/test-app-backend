    "bcrypt": "^5.0.1",
    "fastify": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-cli": "^2.9.1",
    "fastify-cors": "^5.2.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
